      "type": "luma-faq-section",
      "blocks": {
        "faq_item_GTLijJ": {
          "type": "faq_item",
          "settings": {
            "question": "Will this make me \"white\"?",
            "answer": "No. It restores your skin to its brightest, most natural potential. It fades the tan and spots that make you look darker than you are, but it won't bleach your natural DNA."
          }
        },
        "faq_item_X9VBxe": {
          "type": "faq_item",
          "settings": {
            "question": "Can I use this with the Luma Sunscreen?",
            "answer": "You must! Vitamin C and Sunscreen are best friends. Vitamin C fights free radicals, and Sunscreen blocks UV rays. Using them together doubles your protection against aging."
          }
        },
        "faq_item_Pip8tX": {
          "type": "faq_item",
          "settings": {
            "question": "How long until I see results?",
            "answer": "This is science, not magic. You will see a \"glow\" in 1 week. Significant fading of dark spots takes 4–6 weeks of consistent use because that is how long your skin cycle takes to renew."
          }
        }
