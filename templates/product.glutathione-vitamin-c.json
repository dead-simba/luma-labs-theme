    "luma_ingredients_section_wEybY9": {
      "type": "luma-ingredients-section",
      "blocks": {
        "ingredient_Tc4NfW": {
          "type": "ingredient",
          "settings": {
            "title": "Glutathione (The Master Antioxidant)",
            "description": "The gold standard in brightening. It creates a \"depigmenting\" effect by inhibiting the enzyme (Tyrosinase) that produces dark spots. It’s naturally found in your body, making it the safest path to radiance.",
            "show_plus": true
          }
        },
        "ingredient_mj8KjT": {
          "type": "ingredient",
          "settings": {
            "title": "Vitamin C (The Radiance Booster)",
            "description": "We use a stable form of Vitamin C that won't oxidize (turn brown) quickly. It fades surface dullness and boosts collagen, giving you that \"glass skin\" bounce.",
            "show_plus": true
          }
        },
        "ingredient_4bHNb6": {
          "type": "ingredient",
          "settings": {
            "title": "Hyaluronic Acid (The Cushion)",
            "description": "Brightening ingredients can sometimes be drying. We added Hyaluronic Acid to ensure your skin stays hydrated and plump while the actives do their work.",
            "show_plus": true
          }
        },
        "ingredient_BCfQeV": {
          "type": "ingredient",
          "settings": {
            "title": "All Ingredients",
            "description": "The Brighteners & Actives (The Heavy Lifters): Glutathione, Alpha Arbutin, Arbutin, 3-O-Ethyl Ascorbic Acid (Vit C), Sodium Ascorbyl Phosphate (Vit C), Ascorbyl Palmitate (Vit C), Tocopherol Acetate (Vit E).\n\nThe Superfood Extracts (The Antioxidants): Blueberry Extract, Garden Cress Sprout Extract, Grapefruit Extract, Mulberry Extract, Olive Leaf Extract, Pomegranate Flower Extract, Raspberry Extract, Strawberry Extract.\n\nThe Hydrators & Soothers (The Cushion): Sodium Hyaluronate (Hyaluronic Acid), Allantoin, Betaine, Glycerin, Panthenol (Vit B5), Lecithin, Linoleic Acid, Linolenic Acid, Maltodextrin, Sodium Lactate, Sodium Gluconate.\n\nThe Base & Stabilizers (The Architecture): Purified Water, Propanediol, Alcohol, Heptyl Glucoside, Ethylhexylglycerin, Phenoxyethanol, Potassium Sorbate, Sodium Benzoate, Sodium Hydroxide, Xanthan Gum, Perfume.",
            "show_plus": true
          }
        }
      },
