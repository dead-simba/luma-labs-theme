    "luma_ingredients_section_wEybY9": {
      "type": "luma-ingredients-section",
      "blocks": {
        "ingredient_Tc4NfW": {
          "type": "ingredient",
          "settings": {
            "title": "10% LACTIC ACID + 1% SALICYLIC ACID (THE EXFOLIATORS)",
            "description": "A dual-action complex that targets both surface roughness and deep pore congestion. Lactic Acid (AHA) gently dissolves the hard keratin plugs (\"chicken skin\") on the surface to smooth texture instantly. Meanwhile, Salicylic Acid (BHA) is oil-soluble, diving deep into the follicle to clear trapped sebum and prevent the \"red bumps\" (folliculitis) caused by ingrown hairs.",
            "show_plus": true
          }
        },
        "ingredient_mj8KjT": {
          "type": "ingredient",
          "settings": {
            "title": "2% TRANEXAMIC ACID + 4% NIACINAMIDE (THE BRIGHTENERS)",
            "description": "This is your defense against the \"dark dots\" (hyperpigmentation) that scrubs cannot fix. Tranexamic Acid works to inhibit the inflammatory pathway that turns ingrowns into stubborn brown spots. Paired with Niacinamide, it blocks pigment transfer to the surface and repairs the skin barrier, giving legs a \"polished\" and even-toned look.",
            "show_plus": true
          }
        },
        "ingredient_4bHNb6": {
          "type": "ingredient",
          "settings": {
            "title": "5% UREA + CERAMIDES (THE HYDRATORS)",
            "description": "Urea is a \"double agent\"—it intensely hydrates dry skin while simultaneously softening hard, crusty skin cells so they wash away easily. We added a complex of Ceramides (NP, AP, EOP) to immediately seal the skin barrier after exfoliation. This locks in moisture to prevent the \"ashy\" look, without the sticky or greasy feel of traditional creams.",
            "show_plus": true
          }
        },
        "ingredient_BCfQeV": {
          "type": "ingredient",
          "settings": {
            "title": "All Ingredients",
            "description": "Key Actives: Lactic Acid (10%), Urea (5%), Niacinamide (4%), Tranexamic Acid (2%), Salicylic Acid (1%), Ceramide Complex.\n\nBase: Purified Water, Propanediol (Humectant), Glycerin, Xanthan Gum (Texture Enhancer), Phenoxyethanol (Preservative), Ethylhexylglycerin.\nFormulated Without: Fragrance, Essential Oils, Sulfates (SLS/SLES), Parabens, or Artificial Dyes.",
            "show_plus": true
          }
        }
      },
