    "luma_ingredients_section_wEybY9": {
      "type": "luma-ingredients-section",
      "blocks": {
        "ingredient_Tc4NfW": {
          "type": "ingredient",
          "settings": {
            "title": "2% SALICYLIC ACID + ZINC PCA (THE ACNE FIGHTERS)",
            "description": "This is your first line of defense against active breakouts. Salicylic Acid (BHA) is oil-soluble, meaning it dives deep into the sweat glands on your back to dissolve trapped sebum and dry out pimples overnight. We paired it with Zinc PCA to regulate oil production, preventing new \"grease traps\" from forming under your clothes.",
            "show_plus": true
          }
        },
        "ingredient_mj8KjT": {
          "type": "ingredient",
          "settings": {
            "title": "3% TRANEXAMIC ACID + NIACINAMIDE (THE MARK FADERS)",
            "description": "Most body sprays stop at acne; we treat the aftermath. Tranexamic Acid is a powerhouse that targets the stubborn dark spots (PIH) left behind by healed pimples. It works synergistically with Niacinamide to block pigment transfer and reduce redness, ensuring your back looks clear and even-toned in open-back dresses.",
            "show_plus": true
          }
        },
        "ingredient_4bHNb6": {
          "type": "ingredient",
          "settings": {
            "title": "0.5% PIROCTONE OLAMINE (THE FUNGAL DEFENSE)",
            "description": "In Pakistan's humidity, half of \"back acne\" is actually yeast (Malassezia). Antibiotics won't touch it. Piroctone Olamine is a specialized antifungal agent that targets the root cause of those itchy, sweat-induced bumps (\"Garmi Danay\"), clearing the rash that standard acne washes miss.",
            "show_plus": true
          }
        },
        "ingredient_BCfQeV": {
          "type": "ingredient",
          "settings": {
            "title": "All Ingredients",
            "description": "Key Actives: Salicylic Acid (2%), Tranexamic Acid (3%), Niacinamide (4%), Zinc PCA (1%), Piroctone Olamine (0.5%), Allantoin (Soothing Agent).\nBase: Aqua (Water), Propanediol (Non-Sticky Humectant), Betaine, Phenoxyethanol, Ethylhexylglycerin.\nFormulated Without: Benzoyl Peroxide (Bleach), Fragrance, Alcohol Denat (Stinging), Sulfates, or Artificial Dyes.",
            "show_plus": true
          }
        }
      },
